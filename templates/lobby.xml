<fest:template xmlns:fest="http://fest.mail.ru" context_name="context">
    <div class="lobby-box">
        <div class="lobby-box__header"><fest:value>context.name</fest:value></div>
        <div>
            <table class="current-lobby-table">
                <tr>
                    <td class="current-lobby-table__td_choco">TEAM CHOCO</td>
                    <td class="current-lobby-table__td_ginger">TEAM GINGER</td>
                </tr>
                <tr>
                    <td><fest:value>context.team['1'][0]</fest:value></td>
                    <td><fest:value>context.team['0'][0]</fest:value></td>
                </tr>
                <tr>
                    <td><fest:value>context.team['1'][1]</fest:value></td>
                    <td><fest:value>context.team['0'][1]</fest:value></td>
                </tr>
                <tr>
                    <td><fest:value>context.team['1'][2]</fest:value></td>
                    <td><fest:value>context.team['0'][2]</fest:value></td>
                </tr>
                <tr>
                    <td><fest:value>context.team['1'][3]</fest:value></td>
                    <td><fest:value>context.team['0'][3]</fest:value></td>
                </tr>
                <tr>
                    <td><fest:value>context.team['1'][4]</fest:value></td>
                    <td><fest:value>context.team['0'][4]</fest:value></td>
                </tr>
                <tr>
                    <td><fest:value>context.team['1'][5]</fest:value></td>
                    <td><fest:value>context.team['0'][5]</fest:value></td>
                </tr>
            </table>
        </div>
        <div class="button-back">Leave</div>
    </div>
</fest:template>