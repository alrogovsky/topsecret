<fest:template xmlns:fest="http://fest.mail.ru" context_name="lobbies">
    <div class="lobby-box">
        <fest:each iterate="lobbies" index="i" value="top">
            <div class="lobby-box__button">
                <span class="lobby-box__button__left">
                    <fest:value>lobbies[i].attributes.name</fest:value>
                </span>
                <span class="lobby-box__button__right"> players: <fest:value>lobbies[i].attributes.count</fest:value>/6</span>
            </div>
        </fest:each>
    </div>
</fest:template>